generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model user{
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  username    String @unique
  email       String @unique
  password    String
}

model session{
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  userId      String
  score       Int
  createdAt   DateTime @default(now())
  
}

model technical{
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  difficulty  String
  beginning   String
  roundOne    String
  roundTwo    String 
  roundThree  String
  ending      String
}

model behavioral{
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  difficulty  String
  beginning   String
  roundOne    String
  roundTwo    String 
  roundThree  String
  ending      String
}

model leetcode{
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  number      String
  question    String
  difficulty  String
  company     String
}